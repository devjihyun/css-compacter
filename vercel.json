{
  "version": 2,
  "builds": [
    {
      "src": "css-compacter-react/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "installCommand": "cd css-compacter-react && npm install",
  "buildCommand": "cd css-compacter-react && npm run build",
  "outputDirectory": "css-compacter-react/dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ]
}
